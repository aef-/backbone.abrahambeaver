extends ../layouts/admin

block content
  .row
    .col-lg-12
      h1="Experiments"
      ol.breadcrumb
        li
          i.fa.fa-table=" "
            a( href="/" )="Experiments"
        li.active= experiment.getName( )

      if experiment.hasGoals( )
        for goal in experiment.getGoals( )
          h3=goal
          include ../variants/list
      else
        include ../variants/list
